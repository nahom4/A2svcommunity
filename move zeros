class Solution:
    def moveZeroes(self, nums: List[int]) -> None:
        """
        Do not return anything, modify nums in-place instead.
        
        """
        for x in range(len(nums)):
            
            if nums[x] != 0:
                
                while nums[x-1] == 0 and not(x ==0):
                    nums[x-1] = nums[x]
                    nums[x] =0
                    x = x-1
                    
                
                    
