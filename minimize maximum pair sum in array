class Solution:
    def minPairSum(self, nums: List[int]) -> int:
        nums.sort()
        max = 0
        i,j = 0,len(nums)-1
        while i < j:
            if max < nums[i] + nums[j]:
            
                max =  nums[i] + nums[j]
            i+=1
            j-=1
    
        return max
    
